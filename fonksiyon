		cin >> isimler[i];
	}

	cout << "girdiginiz isim1 = " << isimler[0] << endl << "girdiginiz isim2 = " << isimler[1] << endl << "girdiginiz isim3 = " << isimler[2] << endl << "girdiginiz isim4 = " << isimler[3] << endl << "girdiginiz isim5 = " << isimler[4] << endl;
}

